echo -e "0b@MA0b@MAaAaA$(ctext yellow)$(ctext dmagenta).„-,$(ctext dyellow)$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,,;;;;;;;;;;;$(ctext dgreen)\. . |:::|. . .,,,$(ctext dyellow);;;;;;;;|;;/$(ctext reset)"
