echo -e "0b@MA0b@MAaAa$(ctext yellow)$(ctext dmagenta)/$(ctext dyellow)$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;;;;;;;;;;,;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;$(ctext dgreen),,,: |;|. . . . \\$(ctext dyellow);;;;;;;|$(ctext reset)"
echo -e "0b@MA0b@MAaA$(ctext yellow)$(ctext dmagenta)„~$(ctext dyellow)$(tput bold)$(ctext yellow),,;;;;;;;;;;;;;;;;;;;;;$(ctext reset)$(tput dim)$(ctext dgreen)$(ctext yellow),-,,$(tput bold)$(ctext yellow)$(ctext dyellow)$(ctext dyellow);;;;;;;;;;;;;;;;;;;;;;;;;;\;;;;;;;;$(ctext dgreen)|.|;|. . . . .| $(ctext dyellow);;;;;;;|$(ctext reset)"
echo -e "0b@MA0b@MA$(ctext yellow)$(ctext dmagenta)..„~$(ctext dyellow)$(tput bold)$(ctext yellow),,;;;;;;;;;;;;;;;;;;;;;;$(ctext reset)$(ctext yellow)$(tput dim)$(ctext dgreen),-,$(tput bold)$(ctext yellow)$(ctext dyellow)$(ctext dyellow);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,,;;;;;;$(ctext dgreen)| |:|. . . . |\\$(ctext dyellow);;;;;;;|$(ctext reset)"
echo -e "0b@MA0b@MA$(ctext yellow)$(ctext dmagenta).,,$(ctext dyellow)$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;;;;;;;;$(ctext reset)$(ctext yellow)$(tput dim)$(ctext dwhite)/$(tput dim)$(ctext yellow)$(ctext dyellow);;;$(ctext reset)$(ctext yellow)$(ctext dyellow),-,$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;,;;;;;$(ctext dgreen)| |:|. . . .,\|$(ctext dyellow);;,,;;;;;|$(ctext reset)"
