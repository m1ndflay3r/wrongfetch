echo -e "0b@MA0b@MA0b@MA0b@MA0b@MAa$(ctext dblue)$(ctext magenta)(¯,,~-,:$(ctext dwhite) : 0 :,¯°: ,,: :|: :°-: :$(ctext dblue)|$(ctext reset)			$(tput bold)$(ctext white)AND$(ctext magenta): $(ctext green)${RANDOM}$(tput bold)$(ctext white)HuRT $(ctext magenta)/ $(ctext green)${RANDOM}$(tput bold)$(ctext white)YoU$(ctext reset)"
