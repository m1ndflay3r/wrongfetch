echo -e "0b@MAaAa$(ctext yellow)$(ctext dmagenta)../$(ctext dyellow)$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;$(ctext reset)$(ctext yellow)$(tput dim)$(ctext dmagenta),-,a|$(tput dim)$(ctext yellow)$(ctext dyellow);;$(ctext reset)$(ctext yellow)$(ctext dyellow),$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;$(ctext dgreen)| |:|._,-,$(ctext dyellow);;;;;;;;;|;;;;|;;;;;;;;;;;,,,-„_$(ctext reset)"
echo -e "0b@MAaAa$(ctext yellow)$(ctext dmagenta)/$(ctext dyellow)$(tput bold)$(ctext yellow);;;;;;;;;;;;;;;;$(ctext reset)$(ctext yellow)$(tput dim)$(ctext dmagenta),$(ctext dred)-,a.,,$(tput dim)$(ctext yellow)$(ctext dyellow);;$(ctext reset)$(ctext yellow)$(ctext dyellow),$(tput bold);;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;$(ctext dgreen)|.|:|::::,,,~--~,,,\|\|$(ctext dyellow);;;;;|;;;;;;;;;;,-~,,,,~--„______$(ctext reset)"
